       MNJ_MFG_RATIOPLAN_PLYCFLINE det,
       oe_order_headers_all        ooha
 where r.RATIOPLAN_ID = rl.RATIOPLAN_ID
   and ooha.HEADER_ID = r.SALESORDER_ID
   and rl.PLYLINE_ID = det.PLYLINE_ID(+)
   and r.ratioplan_number = :p_pationplnId
   and r.DIVISION_ID = :p_org_id]]>
