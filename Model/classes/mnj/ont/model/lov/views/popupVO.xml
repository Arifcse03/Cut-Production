  <SQLQuery>
    <![CDATA[SELECT BPO_ID,
STN_SEQUENCE,
WIP_ENTITY_ID,
STYLE,
BPO,
ITEM_DESCRIPTION,
ORDERED_QTY,
EXTRA_PER,
CUT_PLAN_NO, --=2019/1106,
(EXTRA_PER - NVL (PREV_PROD, 0)) BELANCE_PRODUCTION,
NVL (PREV_PROD, 0) PREVIOUS_PRODUCTION,
POC_ID
FROM (SELECT B.BPO_ID,
STN_SEQUENCE,
WE.WIP_ENTITY_ID,
H.STYLE,
B.BPO,
l.SYSTEM_ID || H.STYLE || L.COLOR || L.WASH_NAME
ITEM_DESCRIPTION,
BB.STN_QTY ORDERED_QTY,
BB.STN_QTY * ( (RP.EXTRA_CUT_PERCENT / 100) + 1) EXTRA_PER,
(SELECT SUM (NVL (MMRO.PRODUCTION, 0))
FROM MNJ_MFG_RANGE_OBLINE MMRO
WHERE MMRO.BPO_ID = BB.BPO_ID)
PREV_PROD,
RP.RATIOPLAN_NUMBER Cut_Plan_No,
h.POC_ID
FROM XX_OM_POC_H_T H,
XX_OM_POC_L_T L,
XX_OM_BPO_INFO_T B,
MNJ_MFG_RATIOPLAN_BPOS_ALL HE,
MNJ_MFG_RATIOPLAN_STNS_ALL BB,
MNJ_MFG_RATIOPLAN RP,
WIP_ENTITIES WE,
WIP_DISCRETE_JOBS_V WDJ
WHERE H.POC_ID = L.POC_ID
AND B.FOB_ID = L.FOB_ID
AND HE.RATIOPLAN_ID = BB.RATIOPLAN_ID
AND HE.SALE_ORDER_ID = B.BPO_ID
AND RP.RATIOPLAN_ID = HE.RATIOPLAN_ID
AND RP.RATIOPLAN_ID = BB.RATIOPLAN_ID
AND WE.PRIMARY_ITEM_ID = L.ITEM_ID
AND WE.WIP_ENTITY_ID = WDJ.WIP_ENTITY_ID)]]>
  </SQLQuery>
