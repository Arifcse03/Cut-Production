<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PLAN_NO_LOV"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnj.ont.model.lov.views.PLAN_NO_LOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="p_org_id"
    Kind="where"
    Type="java.lang.String">
    <TransientExpression><![CDATA[adf.context.sessionScope.orgId]]></TransientExpression>
  </Variable>
  <Variable
    Name="poc_id"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[select distinct h.ratioplan_number, 
   
       H.RATIO_PLAN_QTY
    
 from MNJ_MFG_RATIOPLAN h, xx_om_poc_h_t pocH,xx_om_poc_L_t pocL
where pocH.poc_id=pocL.poc_id
and h.DIVISION_ID = :p_org_id
and h.System_id=pocL.SYSTEM_ID
and pocH.poc_id=:poc_id]]>
  </SQLQuery>
  <ViewAttribute
    Name="RatioplanNumber"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="RATIOPLAN_NUMBER"
    Expression="RATIOPLAN_NUMBER"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="500"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="20"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="RatioPlanQty"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="RATIO_PLAN_QTY"
    Expression="RATIO_PLAN_QTY"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="20"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
