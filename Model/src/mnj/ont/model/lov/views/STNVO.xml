    Kind="where"
    Type="java.lang.String">
    <TransientExpression><![CDATA[adf.context.sessionScope.orgId]]></TransientExpression>
  </Variable>
  <SQLQuery>
    <![CDATA[Select ooha.HEADER_ID,
       ooha.ORDER_NUMBER,
       MSI.DESCRIPTION,
       msi.INVENTORY_ITEM_ID,
       ooha.ORDER_NUMBER||'/'||oola.line_number STN  ,
       oola.LINE_ID         
  FROM oe_order_headers_all         ooha,
       oe_order_lines_all           oola,
       mtl_system_items             msi,
       org_organization_definitions ood     
 Where oola.HEADER_ID = ooha.HEADER_ID
   and oola.INVENTORY_ITEM_ID = MSI.INVENTORY_ITEM_ID
   and ooha.org_id = ood.operating_unit
   and msi.organization_id = ood.organization_id
   and msi.inventory_item_id = oola.INVENTORY_ITEM_ID
   and oola.LINE_ID = :p_SaleOrderLineID
   and ooha.ORG_ID = :p_org_id]]>
  </SQLQuery>
